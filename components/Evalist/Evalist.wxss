.eval_head {
  width: 750rpx;
  height: 100rpx;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-box-pack: end;
  -ms-flex-pack: end;
  justify-content: flex-end;
  padding: 0rpx 55rpx;
}

.eva_title {
  font-size: 30rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(53, 53, 53, 1);
  margin-right: auto;
}

.eva_mintitle {
  font-size: 24rpx;
  color: rgba(136, 136, 136, 1);
}

.eva_rate {
  font-size: 26rpx;
  color: rgba(69, 208, 174, 1);
}

.eval_wrap {
  width: 100%;
  height: auto;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  padding: 10rpx 47rpx 43rpx 55rpx;
  background-color: rgba(255, 255, 255, 1);
}

.eval_tapWrap {
  width: 100%;
  height: auto;
  overflow: hidden;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  padding: 0rpx 0rpx 15rpx 0rpx;
  border-bottom: 1rpx solid rgba(243, 244, 247, 1);
}

.eval_item {
  overflow: hidden;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  display: inline-block;
  line-height: 40rpx;
  padding: 0rpx 32rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(53, 53, 53, 1);
  background: rgba(69, 208, 174, 0.3);
  border-radius: 10rpx;
  margin-right: 12rpx;
  margin-top: 20rpx;
}

.look_all {
  width: 160rpx;
  height: 36rpx;
  background: rgba(69, 208, 174, 1);
  border-radius: 10rpx;
  margin: 0 auto;
  margin-top: 20rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  line-height: 36rpx;
}

.list {
  width: 100%;
  border-bottom: 1rpx solid rgba(243, 244, 247, 1);
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  padding: 0rpx 0rpx 12rpx 0rpx;
}

.list_head {
  width: 100%;
  height: 64rpx;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
}

.list_head_avater {
  width: 42rpx;
  height: 42rpx;
  border-radius: 50%;
}

.list_head_name {
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(136, 136, 136, 1);
  margin-left: 10rpx;
}

.list_content {
  width: 100%;
  height: auto;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  padding: 9rpx 0rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(136, 136, 136, 1);
}

.list_img_wrap {
  width: 100%;
  height: auto;
  overflow: hidden;
}

.list_img {
  width: 145rpx;
  height: 145rpx;
  border-radius: 10rpx;
  float: left;
  margin-right: 20rpx;
  margin-bottom: 12rpx;
}

.list_tip {
  width: 100%;
  font-size: 16rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(136, 136, 136, 1);
}
.nodata{
  width: 100%;
  height: 400rpx;
  font-size: 24rpx;
  color: rgba(153, 153, 153, 1);
  text-align: center;
  line-height: 400rpx;
}