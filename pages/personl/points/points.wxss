    align-items: flex-start;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between;
    margin-top: 10rpx
}

.list_wrap .list_content .list_item .item_res .res_left {
    width: 500rpx;
    font-size: 26rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #3f3f3f
}

.list_wrap .list_content .list_item .item_res .res_score {
    font-size: 32rpx;
