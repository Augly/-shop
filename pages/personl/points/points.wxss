.head_group .pro_group .title {
    font-size: 30rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #fff;
    margin: 0 18rpx
}

.head_group .pro_group .mintitle {
    font-size: 24rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #fff;
    margin-left: 26rpx;
    margin-right: auto
}

.head_group .pro_group .pro_now {
    height: 100%;
    background: rgba(255, 255, 255, 0.45);
    border-radius: 10rpx;
    position: absolute
}

.list_wrap {
    width: 100%;
    height: auto;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    padding: 0 24rpx
}

.list_wrap .list_head {
    width: 100%;
    height: 74rpx;
    font-size: 26rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #888;
    line-height: 74rpx;
    text-indent: 23rpx
}

.list_wrap .list_content {
    width: 100%;
    height: auto;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    padding: 0 23rpx 24rpx 23rpx;
    background: #fff;
    border-radius: 10rpx;
