.head_group .point_group {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    font-size: 36rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #fff
}

.head_group .point_group .point_number {
    font-size: 52rpx;
    font-family: Helvetica;
    font-weight: 400;
    color: #fff
}

.head_group .grade_group {
    font-size: 24rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #fff;
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center
}

.head_group .grade_group .grade_number {
    display: inline-block;
    padding: 0 18rpx;
    height: 33rpx;
    background: #fff;
    border: 1rpx solid #fff;
    border-radius: 10rpx;
    font-size: 30rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    line-height: 33rpx;
    color: #45d0ae;
    margin-left: 30rpx
