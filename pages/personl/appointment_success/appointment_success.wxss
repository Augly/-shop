page {
  position: relative;
  height: 100%;
}
page .wrap {
  position: relative;
  height: 100%;
  width: 100%;
  background-color: #ffffff;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
page .wrap .iconchenggong {
  font-size: 106rpx;
  color: #45d0ae;
}
page .wrap .tip {
  width: 100%;
  height: auto;
  display: flex;
  align-items: center;
  font-size: 60rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: #45d0ae;
  justify-content: center;
  margin-top: 25rpx;
  margin-bottom: 60rpx;
}
page .wrap .tip_content {
  width: 450rpx;
  height: 40rpx;
  font-size: 18rpx;
  font-family: PingFangSC-Light;
  font-weight: 300;
  color: #353535;
  margin: 0 auto;
  text-align: center;
}
page .status {
  font-size: 30rpx;
  font-family: PingFangSC-Light;
  font-weight: 300;
  color: #3f3f3f;
  display: flex;
  align-items: center;
}
page .status .status_number {
  color: #45d0ae;
}
page .time_group {
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  margin-top: 20rpx;
}
page .time_group .time_group_group {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 27rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #3f3f3f;
}
page .time_group .time_group_group .time_group_group_item {
  display: inline-block;
  padding: 0rpx 6rpx;
  height: 34rpx;
  background: #3f3f3f;
  border-radius: 5rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #ffffff;
  line-height: 34rpx;
  margin-left: 5rpx;
  margin-right: 5rpx;
}
page .personl_group {
  width: 650rpx;
  height: auto;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-wrap: wrap;
}
page .personl_group .personl_item {
  width: 92rpx;
  height: 140rpx;
  margin-left: 18rpx;
  margin-right: 18rpx;
}
page .personl_group .personl_item .personl_logo {
  width: 92rpx;
  height: 92rpx;
  border-radius: 50%;
  background-color: red;
}
page .sure_button {
  width: 672rpx;
  height: 104rpx;
  background: #45d0ae;
  border-radius: 20rpx;
  margin: 0 auto;
  margin-top: 94rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: #ffffff;
  text-align: center;
  line-height: 104rpx;
}
page .note_tip {
  font-size: 24rpx;
  font-family: PingFangSC-Light;
  font-weight: 300;
  color: #aaaaaa;
  text-align: center;
  margin-top: 50rpx;
}
.look_around_title {
  width: 702rpx;
  height: 70rpx;
  background: #ffffff;
  border-radius: 10rpx;
  margin: 0 auto;
  margin-top: 14rpx;
  font-size: 30rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #45d0ae;
  text-align: center;
  line-height: 70rpx;
}
.res {
  width: 100%;
  height: auto;
  background: #ffffff;
  box-sizing: border-box;
  padding: 0rpx 30rpx;
  margin-top: 18rpx;
}
.res .res_item {
  width: 100%;
  height: 70rpx;
  border-bottom: 1rpx solid #f3f4f7;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.res .res_item .item_title {
  font-size: 30rpx;
  font-family: PingFangSC-Light;
  font-weight: 300;
  color: #939393;
  margin-right: auto;
}
.res .res_item .item_mintitle {
  font-size: 30rpx;
  font-family: PingFangSC-Light;
  font-weight: 300;
  color: #3f3f3f;
}
.res .res_item .iconjiantou {
  font-size: 24rpx;
  font-family: PingFangSC-Light;
  font-weight: 300;
  color: #3f3f3f;
  margin-left: 10rpx;
}
