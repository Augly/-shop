.list_item {
    width: 100%;
    height: 135rpx;
    background: #fff;
    border-radius: 10rpx;
    margin-top: 14rpx;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    padding: 0 26rpx;
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center
}

.list_item .avter {
    width: 80rpx;
    height: 80rpx;
    background: red;
    border-radius: 50%
}

.list_item .item_res {
    height: 80rpx;
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -ms-flex-direction: column;
    flex-direction: column;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between;
    margin-left: 26rpx
}

.list_item .item_res .title {
    font-size: 32rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #353535
}

.list_item .item_res .data {
    font-size: 22rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #888
}