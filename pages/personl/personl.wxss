.grid_wrap {
  width: 100%;
  height: auto;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  padding: 0rpx 24rpx 24rpx 24rpx;
  overflow: hidden;
}

.grid_item {
  width: 218rpx;
  height: 218rpx;
  background: rgba(255, 255, 255, 1);
  float: left;
  margin-top: 20rpx;
  margin-right: 24rpx;
  box-sizing: border-box;
  padding: 33rpx 0rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.grid_item:nth-child(3n) {
  margin-right: 0;
}

.other_wrap {
