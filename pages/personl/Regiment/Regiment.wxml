<view class="tab_group" catchtap='tabChange'>
    <block wx:for="{{tab}}" wx:for-index="index" wx:for-item="item" wx:key="index">
        <view class="tab_item {{index==tab_index?'tab_item_active':''}}" data-id='{{index}}'>
            {{item.title}}
        </view>
    </block>
</view>
