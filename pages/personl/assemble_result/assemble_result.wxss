page {
  position: relative;
  height: 100%;
}
page .wrap {
  position: relative;
  height: 100%;
  width: 100%;
  background-color: #ffffff;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
page .wrap .iconchenggong {
  font-size: 106rpx;
  color: #45d0ae;
}
page .wrap .tip {
  width: 100%;
  height: auto;
  display: flex;
  align-items: center;
  font-size: 60rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: #45d0ae;
  justify-content: center;
  margin-top: 25rpx;
  margin-bottom: 60rpx;
}
page .wrap .tip_content {
  width: 450rpx;
  height: 40rpx;
  font-size: 18rpx;
  font-family: PingFangSC-Light;
  font-weight: 300;
  color: #353535;
  margin: 0 auto;
  text-align: center;
}
page .wrap .tip_border {
  width: 150rpx;
  height: 48rpx;
  border: 1rpx solid #45d0ae;
  font-size: 24rpx;
  color: #45d0ae;
  text-align: center;
  line-height: 48rpx;
  border-radius: 24rpx;
  margin-top: 150rpx;
}
page .look_around_title {
  width: 702rpx;
  height: 70rpx;
  background: #ffffff;
  border-radius: 10rpx;
  margin: 0 auto;
  margin-top: 14rpx;
  font-size: 30rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #45d0ae;
  text-align: center;
  line-height: 70rpx;
}
